#usda 1.0
(
    defaultPrim = "M_Glass_Frosted"
    upAxis = "Z"
)

def Material "M_Glass_Frosted"
{
    token outputs:surface.connect = </M_Glass_Frosted/PBRShader.outputs:surface>

    def Shader "PBRShader"
    {
        uniform token info:id = "UsdPreviewSurface"
        color3f inputs:diffuseColor = (0.104616486, 0.38642943, 0.30946892)
        float inputs:opacity = 0.69803923
        int inputs:useSpecularWorkflow = 1
        token outputs:surface
    }
}

