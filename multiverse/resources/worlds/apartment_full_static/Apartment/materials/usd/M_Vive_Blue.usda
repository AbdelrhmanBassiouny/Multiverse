#usda 1.0
(
    defaultPrim = "M_Vive_Blue"
    upAxis = "Z"
)

def Material "M_Vive_Blue"
{
    token outputs:surface.connect = </M_Vive_Blue/PBRShader.outputs:surface>

    def Shader "PBRShader"
    {
        uniform token info:id = "UsdPreviewSurface"
        color3f inputs:diffuseColor = (0.012286488, 0.014443844, 0.045186203)
        float inputs:opacity = 1
        int inputs:useSpecularWorkflow = 1
        token outputs:surface
    }
}

