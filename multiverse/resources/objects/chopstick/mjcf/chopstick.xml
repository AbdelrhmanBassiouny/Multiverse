<mujoco model="wooden_log">
	<compiler angle="radian" meshdir="../meshes/" texturedir="../meshes/textures/" />
	<asset>
		<mesh name="Chopstick" file="obj/Chopstick.obj" />
		<mesh name="UCX_Chopstick_001" file="stl/UCX_Chopstick_001.stl" />
	</asset>
	<default>
		<default class="chopstick_visual">
			<geom contype="0" conaffinity="0" rgba="0.9 0.9 0.9 1.0"/>
		</default>
		<default class="chopstick_collision">
			<geom rgba="1.0 0.0 0.0 0.0" />
		</default>
		<default class="chopstick_reference">
			<geom contype="0" conaffinity="0" rgba="1.0 0.0 0.0 1.0" />
		</default>
	</default>
	<worldbody>
		<body name="chopstick" gravcomp="1">
			<freejoint />
			<geom type="mesh" mesh="Chopstick" class="chopstick_visual" />
			<geom type="mesh" mesh="UCX_Chopstick_001" class="chopstick_collision"/>
		</body>
	</worldbody>
	<worldbody>
		<body name="chopstick_ref" mocap="true">
			<geom type="mesh" mesh="Chopstick" class="chopstick_reference" />
		</body>
	</worldbody>
	<equality>
		<weld body1="chopstick" body2="chopstick_ref" torquescale="10"/>
	</equality>
</mujoco>